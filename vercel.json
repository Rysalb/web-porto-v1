{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { 
      "src": "/flutter_service_worker.js",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/manifest.json",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "flutter build web --release",
  "outputDirectory": "build/web"
}